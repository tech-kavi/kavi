{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Company",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "company_id": {
      "type": "uid"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "meta": {
      "type": "text"
    },
    "industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::industry.industry",
      "mappedBy": "companies"
    },
    "sub_industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-industry.sub-industry",
      "inversedBy": "companies"
    },
    "watchlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::watchlist.watchlist",
      "mappedBy": "company"
    },
    "ipo": {
      "type": "enumeration",
      "enum": [
        "IPO",
        "DRHP"
      ]
    },
    "ownership_type": {
      "type": "enumeration",
      "enum": [
        "Public",
        "Private"
      ],
      "required": true
    },
    "secondary_articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "secondary_companies"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "primary_companies"
    }
  }
}
