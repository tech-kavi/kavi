{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "bookmarks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bookmark.bookmark",
      "mappedBy": "bookmarked_by"
    },
    "watchlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::watchlist.watchlist",
      "mappedBy": "watchlisted_by"
    },
    "name": {
      "type": "string"
    },
    "orgID": {
      "type": "biginteger",
      "required": true
    },
    "slots": {
      "type": "integer"
    },
    "slotFilled": {
      "type": "integer"
    },
    "expiry": {
      "type": "date"
    },
    "currentToken": {
      "type": "text",
      "private": true
    },
    "read_articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::read-article.read-article",
      "mappedBy": "user"
    },
    "liked_articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::liked-article.liked-article",
      "mappedBy": "user"
    },
    "LinkedinURL": {
      "type": "string",
      "private": true
    },
    "logged_in": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::logged-in.logged-in",
      "mappedBy": "user"
    }
  }
}
