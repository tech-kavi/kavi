{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "bookmarks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bookmark.bookmark",
      "mappedBy": "bookmarked_by"
    },
    "watchlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::watchlist.watchlist",
      "mappedBy": "watchlisted_by"
    },
    "first_name": {
      "type": "string"
    },
    "orgID": {
      "type": "biginteger",
      "required": true
    },
    "slots": {
      "type": "integer"
    },
    "slotFilled": {
      "type": "integer"
    },
    "expiry": {
      "type": "datetime"
    },
    "currentToken": {
      "type": "text",
      "private": true
    },
    "liked_articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::liked-article.liked-article",
      "mappedBy": "user"
    },
    "linkedinurl": {
      "type": "string",
      "private": true,
      "regex": "(http(s?)://)?(www\\.)?linkedin\\.([a-z])+/(in/)([A-Za-z0-9]+)+/?"
    },
    "logged_in": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::logged-in.logged-in",
      "mappedBy": "user"
    },
    "disliked_articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::disliked-article.disliked-article",
      "mappedBy": "user"
    },
    "last_name": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "last_login": {
      "type": "datetime"
    },
    "highlights": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::highlight.highlight",
      "mappedBy": "user"
    },
    "token": {
      "type": "string",
      "private": true
    },
    "articlesOpenedToday": {
      "type": "component",
      "repeatable": true,
      "component": "open-details.open-details"
    },
    "DailyLimit": {
      "type": "integer",
      "default": 30
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Trial",
        "Subscriber"
      ],
      "default": "Subscriber",
      "required": true
    },
    "TotalLimit": {
      "type": "integer",
      "default": 100
    },
    "OpensToday": {
      "type": "integer"
    },
    "special_articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "allowed_users"
    }
  }
}
